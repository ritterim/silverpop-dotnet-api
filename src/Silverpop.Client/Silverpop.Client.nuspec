<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
   <id>silverpop-dotnet-api</id>
    <title>Silverpop .NET API</title>
    <version>2.0.4</version>
    <authors>Ritter Insurance Marketing</authors>
    <description>This is a .NET API wrapper for Silverpop Transact XML email sending.</description>
    <projectUrl>https://github.com/ritterim/silverpop-dotnet-api</projectUrl>
    <licenseUrl>https://github.com/ritterim/silverpop-dotnet-api/blob/master/LICENSE.txt</licenseUrl>
    <tags>silverpop api</tags>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <group targetFramework="net461">
        <dependency id="Newtonsoft.Json" version="9.0.1" />
        <dependency id="SSH.NET" version="2013.1.27" />
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="System.Configuration.ConfigurationManager" version="4.4.0" />
        <dependency id="Newtonsoft.Json" version="9.0.1" />
        <dependency id="SSH.NET" version="2013.1.27" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin\Release\net461\Silverpop.*.dll" target="lib\net461" />
    <file src="bin\Release\netstandard2.0\Silverpop.*.dll" target="lib\netstandard2.0" />
  </files>
</package>
